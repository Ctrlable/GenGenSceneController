<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable sendEvents="yes">
      <name>CurScreen</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Button</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>ui1</dataType>
	  <allowedValueRange>
        <minimum>1</minimum>
        <maximum>10</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Label</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Font</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue>Normal</defaultValue>
      <allowedValueList>
        <allowedValue>Normal</allowedValue>
        <allowedValue>Compressed</allowedValue>
        <allowedValue>Inverted</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Align</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue>Center</defaultValue>
      <allowedValueList>
        <allowedValue>Left</allowedValue>
        <allowedValue>Center</allowedValue>
        <allowedValue>Right</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Mode</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Timeout</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>boolean</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_ForceClear</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>boolean</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Timeout_Enable</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>boolean</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Timeout_Screen</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Timeout_Time</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <defaultValue>30</defaultValue>
      <dataType>ui4</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Lines</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>ui1</dataType>
	  <allowedValueRange>
        <minimum>5</minimum>
        <maximum>10</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_TemperatureDevice</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>ui2</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_PresetLanguage</name>
	  <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>ui1</dataType>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>SetScreen</name>
      <argumentList>
        <argument>
          <name>Screen</name>
          <direction>in</direction>
          <relatedStateVariable>CurScreen</relatedStateVariable>
        </argument>
        <argument>
          <name>Timeout</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Timeout</relatedStateVariable>
        </argument>
        <argument>
          <name>ForceClear</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_ForceClear</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>UpdateCustomLabel</name>
      <argumentList>
        <argument>
          <name>Screen</name>
          <direction>in</direction>
          <relatedStateVariable>CurScreen</relatedStateVariable>
        </argument>
        <argument>
          <name>Button</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Button</relatedStateVariable>
        </argument>
        <argument>
          <name>Label</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Label</relatedStateVariable>
        </argument>
        <argument>
          <name>Font</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Font</relatedStateVariable>
        </argument>
        <argument>
          <name>Align</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Align</relatedStateVariable>
        </argument>
        <argument>
          <name>Mode</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Mode</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPresetLanguage</name>
      <argumentList>
        <argument>
          <name>Language</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_PresetLanguage</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>UpdateTemperatureDevice</name>
      <argumentList>
        <argument>
          <name>Screen</name>
          <direction>in</direction>
          <relatedStateVariable>CurScreen</relatedStateVariable>
        </argument>
        <argument>
          <name>TemperatureDevice</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_TemperatureDevice</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetBacklight</name>
      <argumentList>
        <argument>
          <name>Screen</name>
          <direction>in</direction>
          <relatedStateVariable>CurScreen</relatedStateVariable>
        </argument>
        <argument>
          <name>Timeout</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Timeout_Time</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetScreenTimeout</name>
      <argumentList>
        <argument>
          <name>Screen</name>
          <direction>in</direction>
          <relatedStateVariable>CurScreen</relatedStateVariable>
        </argument>
        <argument>
          <name>TimeoutEnable</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Timeout_Enable</relatedStateVariable>
        </argument>
        <argument>
          <name>TimeoutScreen</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Timeout_Screen</relatedStateVariable>
        </argument>
        <argument>
          <name>timeoutSeconds</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Timeout_Time</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetNumLines</name>
      <argumentList>
        <argument>
          <name>Screen</name>
          <direction>in</direction>
          <relatedStateVariable>CurScreen</relatedStateVariable>
        </argument>
        <argument>
          <name>Lines</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Lines</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
